{
  "VERSION": {
    "mapping_file": "1.0.0",
    "mapping_reader": "1.1.0"
  },
  "IDENTIFIER_CONSTANTS": {
    "target_core": "webanno.custom.core",
    "target_modifier": "webanno.custom.modifier",
    "target_qualifier": "webanno.custom.qualifier",
    "target_segment": "de.tudarmstadt.ukp.dkpro.core.api.segmentation.type",
    "source_all": "de.averbis.types.health",
    "source_segment": "de.averbis.extraction.types"
  },
  "MAPPING": {
    "IDENTIFIER": {
      "target_default": "#target_core",
      "source_default": "#source_all"
    },
    "ENTRIES": {
      "Age": {
        "layer": {},
        "features": {
          "kind": "$DATE"
        }
      },
      "Contact": {
        "layer": {},
        "features": {
          "kind": "kind"
        }
      },
      "Date": {
        "layer": {},
        "features": {
          "kind": "kind",
          "day": "day",
          "month": "month",
          "year": "year"
        }
      },
      "ID": {
        "layer": {},
        "features": {
          "kind": "kind"
        }
      },
      "Location": {
        "layer": {},
        "features": {
          "kind": "kind"
        }
      },
      "Name": {
        "layer": {},
        "features": {
          "kind": "kind"
        }
      },
      "PHIOther": {
        "layer": {},
        "features": {
          "kind": "$PHI_OTHER"
        }
      },
      "Profession": {
        "layer": {},
        "features": {
          "kind": "$PROFESSION"
        }
      },
      "Sentence": {
        "layer": {
          "#target_segment.Sentence": "#source_segment.Sentence"
        },
        "features": {}
      }
    }
  }
}